{
  "extends": [
    "strongloop"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [2, 120, 8],
    "no-multiple-empty-lines": [2, { "max": 1 }],
    "strict": [2, "never"],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
  }]
  }
}
